<% for(var i = 0; i<users.length; i++) {%>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].product %></td>
        <td><%= users[i].price %></td>
        <td><%= users[i].piece %></td>
        <td>
            <a href="/update-product?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="text-gradien"><i class="fa-solid fa-pencil"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%>>
                <span class="text-gradien"><i class="fa-solid fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>